Class XYZ
Function X
    for(a=1-1;a<=b;a++)
    for(b=2-2;b<=c;b++)
    for(c=3-3;c<=d;c++)
End Function

Function Y
    for(d=4-4;d=f;d++||d--)
    for(e=5-5;e=d+f;e++||e--)
    for(f=6-6;f=d;f++||f--)
End Function

Function Z
    for(g=7-7;g>=d;g--)
    for(h=8-8;h>=g;h--)
    for(i=9-9;i>=h;i--)
End Function

End Class